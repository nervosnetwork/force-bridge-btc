import basic;

table MintXTWitness {
    spv_proof: Bytes,
    cell_dep_index_list: Bytes,
}

table BTCSPVProof {
    version: Uint32,
    vin: Bytes,
    vout: Bytes,
    locktime: Uint32,
    tx_id: Byte32,
    index: Uint64,
    headers: Bytes,
    intermediate_nodes: Bytes,
    funding_output_index: Uint32,
    funding_input_index: Uint32,
}
